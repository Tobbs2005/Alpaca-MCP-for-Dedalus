{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "alpaca-mcp-for-dedalus",
  "title": "Alpaca MCP Server (Dedalus)",
  "description": "Alpaca Trading API integration via Dedalus MCP (Streamable HTTP)",
  "repository": {
    "url": "https://github.com/Tobbs2005/Alpaca-MCP-for-Dedalus",
    "source": "github"
  },
  "version": "1.0.12",
  "packages": [
    {
      "registryType": "pypi",
      "identifier": "alpaca-mcp-server",
      "version": "1.0.12",
      "transport": {
        "type": "streamable-http",
        "port": 8000,
        "path": "/mcp"
      },
      "environmentVariables": [
        {
          "name": "ALPACA_API_KEY",
          "description": "Alpaca Trading API key",
          "isRequired": true,
          "isSecret": true,
          "format": "string"
        },
        {
          "name": "ALPACA_SECRET_KEY",
          "description": "Alpaca Trading API secret key",
          "isRequired": true,
          "isSecret": true,
          "format": "string"
        }
      ]
    }
  ]
}
